$.get('/search?max-results=200', function(e) {
  var a = $(e).find('.Blog .wp-block-loop.block-loop-row'),random_item=''
  if (a.length) {
    var b = a.length,
      r = 1,
      random_html = document.querySelector('#random')
    if (a.length < r) r = a.length
    let arr = []
    do {
      let num = Math.floor(Math.random() * b)
      arr.push(num)
      arr = arr.filter((item, index) => {
        return arr.indexOf(item) === index
      })
    } while (arr.length < r)
    arr.forEach(function(i) {
      var random_url = $(a[i]).find('.post-thumbnail a').attr('href'),
        str = '<a href="'+random_url+'" title="Nghe ngẫu nhiên"><i class="btn-play-wrap mini"><button class="btn-play"></button></i> <span>Random</span></a>'
      random_item += str
      random_html.innerHTML = random_item
    })
  }
})
//]]></script>
<b:if cond='data:view.isHomepage'>
<script>//<![CDATA[
$.get('/search?max-results=200', function(e) {
  var a = $(e).find('.Blog .wp-block-loop.block-loop-row'),random_item=''
  if (a.length) {
    var b = a.length,
      r = 10,
      random_html = document.querySelector('#random-posts')
    if (a.length < r) r = a.length
    let arr = []
    do {
      let num = Math.floor(Math.random() * b)
      arr.push(num)
      arr = arr.filter((item, index) => {
        return arr.indexOf(item) === index
      })
    } while (arr.length < r)
    arr.forEach(function(i) {
      var random_full = $(a[i]).find('.block-loop-items').html(),
          str = '<div>'+random_full+'</div>'
      random_item += random_full
      random_html.innerHTML = random_item
	  $(random_html).removeClass('loading')
    })
  }
})
//]]></script> 
